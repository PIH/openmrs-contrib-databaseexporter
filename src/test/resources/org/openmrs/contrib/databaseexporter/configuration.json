{
	"sourceDatabaseCredentials": {
		"url": "jdbc:mysql://localhost:3306/openmrs_rwink?autoReconnect=true&useUnicode=true&characterEncoding=UTF-8",
		"user": "root",
		"password": "root"
	},
	"targetLocation": "/home/mseaton/Desktop/TestExport.sql",
	"batchSize":10000,
	"tableFilter": {
		"includeSchema": ["*"],
		"includeData": ["*"],
		"excludeData": [
			"cohort", "cohort_member", "concept_proposal*", "dataquality_tally",
			"field", "form_field", "form2program", "formentry*", "hl7_in_*", "groovy_scripts",
			"metadatasharing_*", "name_phonetics", "person_merge_log", "reporting_*",
			"report_object", "report_schema_xml", "serialized_object",
			"sync_import", "sync_record", "sync_server_record", "user_property"
		]
	},
	"rowFilters": [
		{
			"@class" : "org.openmrs.contrib.databaseexporter.filter.PatientFilter",
			"filterQueries": [
				{
					"@class" : "org.openmrs.contrib.databaseexporter.filter.query.PatientAgeFilterQuery",
					"numberPerAgeRange": 3,
					"ageRanges": [
						{"maxAge": 2},
						{"minAge": 3, "maxAge": 10},
						{"minAge": 11, "maxAge": 15},
						{"minAge": 16, "maxAge": 30},
						{"minAge": 31, "maxAge": 60},
						{"minAge": 61}
					]
				}
			]
		}
	],
	"rowTransforms": [
		{
			"@class": "org.openmrs.contrib.databaseexporter.transform.UserTransform",
			"limitToUsers": [12],
			"passwordReplacement": "Test1234"
		},
		{
			"@class" : "org.openmrs.contrib.databaseexporter.transform.GlobalPropertyTransform",
			"replacements": {
				"scheduler.password": "Test1234"
			}
		},
		{
			"@class": "org.openmrs.contrib.databaseexporter.transform.IdentifierTransform"
		},
		{
			"@class": "org.openmrs.contrib.databaseexporter.transform.LocationTransform",
			"keepOnlyLocations": [1,25,26,27,28.29]
		},
		{
			"@class": "org.openmrs.contrib.databaseexporter.transform.PersonAddressTransform"
		},
		{
			"@class": "org.openmrs.contrib.databaseexporter.transform.PersonNameTransform"
		},
		{
			"@class": "org.openmrs.contrib.databaseexporter.transform.RwandaAddressHierarchyTransform"
		},
		{
			"@class": "org.openmrs.contrib.databaseexporter.transform.ScrambleStatesInWorkflowTransform",
			"workflowToScramble": 9,
			"possibleStates": [247,248,249]
		},
		{
			"@class" : "org.openmrs.contrib.databaseexporter.transform.SimpleReplacementTransform",
			"tableAndColumnList": ["obs.value_text"],
			"replacement": "Value replaced during de-identification ${obs_id}"
		},
		{
			"@class" : "org.openmrs.contrib.databaseexporter.transform.SimpleReplacementTransform",
			"tableAndColumnList": ["form.xslt","form.template"],
			"replacement": "Values removed during database export"
		}
	]
}